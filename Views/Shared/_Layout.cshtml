<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link crossorigin="anonymous" href="../../Content/skeleton.css" media="all" rel="stylesheet"/>
    <link crossorigin="anonymous" href="../../Content/site.min.css" media="all" rel="stylesheet" />

</head>
<body>

<header>
    <h1>LGOG AND MENY</h1>
</header>
<!----   FULL LIQUID     -->
<div class="container-liquid">
    <div class="row">
        <div class="one-whole column" style="background-color: #aaa;"> 100% </div>
    </div>
</div>
<div class="container-liquid">
    <div class="row">
        <div class="one-half column" style="background-color: #ffa500;"> 50% </div>
        <div class="one-half column" style="background-color: #ff00ff;"> 50% </div>
    </div>
</div>
<div class="container-liquid">
    <div class="row">
        <div class="one-third column" style="background-color: #00ff7f;"> 33.33% </div>
        <div class="one-third column" style="background-color: #0000ff; color: #fff;"> 33.33% </div>
        <div class="one-third column" style="background-color: #2e8b57;"> 33.33% </div>
    </div>
</div>
<div class="container-liquid">
    <div class="row">
        <div class="one-sixth column" style="background-color: #ffa500;"> Kommer aldrig ske? </div>
        <div class="one-sixth column" style="background-color: #ff00ff;"> Kommer aldrig ske? </div>
        <div class="row">
            <div class="two-thirds column" style="background-color: #5f9ea0;"> 33.33% </div>            
        </div>
    </div>
</div>


<!----   FULL WIDTH     -->
<div class="container-full-width static-container-height center-background">
    <div class="container">
        <div class="row">
            <div class="one-whole column">
                <h2>Från strategi till effekt</h2>
            </div>
        </div>
    </div>
</div>

<div class="container-full-width static-container-half-height">
    <div class="container">
        <div class="row static-container-height">
            <div class="row">
                <div class="one-half column" style="background-color: #ffa500;">50% </div>
                <div class="one-half column" style="background-color: #ff00ff;">
                    <div class="row static-container-half-height">
                        <div class="one-half column" style="background-color: #9370db;">50% </div>
                        <div class="one-half column" style="background-color: #a0522d;">50%</div>
                        <div class="one-whole column" style="background-color: #ccc;">50%</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@RenderBody()
</body>
</html>
